# Development Environment Configuration
# SmartAir City - Frontend

# ============================================
# AIR QUALITY API (Port 5182)
# ============================================
REACT_APP_AIR_API_URL=http://localhost:5182
REACT_APP_AIR_WS_URL=ws://localhost:5182

# ============================================
# CORE API - Devices & Users (Port 5183)
# ============================================
REACT_APP_CORE_API_URL=http://localhost:5183

# ============================================
# GENERAL SETTINGS
# ============================================
REACT_APP_ENV=development
REACT_APP_AUTO_REFRESH_INTERVAL=30000
REACT_APP_WS_RECONNECT_INTERVAL=5000
REACT_APP_API_TIMEOUT=10000

# ============================================
# FEATURE FLAGS
# ============================================
REACT_APP_ENABLE_WEBSOCKET=true
REACT_APP_ENABLE_AUTH=false
REACT_APP_DEBUG_MODE=true
